{
  "name": "dotfiles-syntax-highlighting",
  "displayName": "Dotfiles Syntax Highlighting",
  "version": "0.0.1",
  "publisher": "dotiful",
  "description": "Shellscript syntax highlighting for common dotfiles.",
  "categories": [
    "Programming Languages"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#333333",
    "theme": "dark"
  },
  "preview": true,
  "bugs": {
    "url": "https://github.com/dotiful/vscode-dotfiles-syntax/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dotiful/vscode-dotfiles-syntax"
  },
  "engines": {
    "vscode": "*"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "contributes": {
    "languages": [
      {
        "id": "shellscript",
        "aliases": [
          "Shell Script"
        ],
        "extensions": [
          "ackrc",
          "aliases",
          "antigen",
          "bash_prompt",
          "brew",
          "cshrc",
          "curlrc",
          "dircolors",
          "editorconfig",
          "env",
          "envrc",
          "exports",
          "extra",
          "functions",
          "git",
          "gitattributes",
          "gitconfig",
          "gitignore",
          "gitmodules",
          "hushlogin",
          "inputrc",
          "jshintignore",
          "npmignore",
          "npmrc",
          "osx",
          "packages",
          "path",
          "pkginit",
          "screenrc",
          "symlink",
          "wgetrc",
          "xsessionrc",
          "zpreztorc"
        ],
        "filenames": [
          "symlink",
          "zlogin",
          "zlogout",
          "zpreztorc",
          "zprofile",
          "zshenv",
          "zshrc"
        ],
        "firstLine": "^#\\s*compdef"
      }
    ]
  },
  "homepage": "https://github.com/dotiful/vscode-dotfiles-syntax/blob/master/README.md",
  "scripts": {
    "build-package": "vsce package",
    "release": "release-it"
  },
  "devDependencies": {
    "release-it": "^12.3.0",
    "vsce": "^1.62.0"
  }
}
