{
  "name": "dotfiles-syntax-highlighting",
  "displayName": "Dotfiles Syntax Highlighting",
  "version": "0.0.1",
  "publisher": "dotiful",
  "description": "Shellscript syntax highlighting for common dotfiles.",
  "categories": [
    "Programming Languages"
  ],
  "icon": "assets/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/dotiful/vscode-dotfiles-syntax"
  },
  "engines": {
    "vscode": "^1.35.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "contributes": {
    "languages": [{
      "id": "shellscript",
      "aliases": ["shellscript"],
      "extensions": [
        "ackrc",
        "aliases",
        "antigen",
        "bash_aliases",
        "bash_profile",
        "bash_prompt",
        "bashrc",
        "brew",
        "cshrc",
        "curlrc",
        "dircolors",
        "editorconfig",
        "env",
        "envrc",
        "eslintignore",
        "exports",
        "extra",
        "functions",
        "git",
        "gitattributes",
        "gitconfig",
        "gitignore",
        "gitmodules",
        "hushlogin",
        "inputrc",
        "jshintignore",
        "npmignore",
        "npmrc",
        "osx",
        "packages",
        "path",
        "pkginit",
        "profile",
        "screenrc",
        "symlink",
        "zlogin",
        "zlogout",
        "zprofile",
        "zshenv",
        "zshrc",
        "zsh-theme",
        "zpreztorc",
        "xsessionrc",
        "wgetrc"
      ],
      "filenames": [
        "symlink",
        "zshrc",
        "zshenv",
        "zlogin",
        "zlogout",
        "zprofile",
        "zpreztorc"
      ],
      "configuration": "./language-configuration.json"
    }]
  },
  "homepage": "https://github.com/dotiful/vscode-dotfiles-syntax/blob/master/README.md"
}
